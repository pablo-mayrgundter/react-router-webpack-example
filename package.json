{
  "name": "react-router-webpack-example-app",
  "version": "1.0.0",
  "description": "Example using react, react-router, webpack and mui components",
  "repository": {
    "type": "git",
    "url": "https://github.com/pablo-mayrgundter/react-router-webpack-example"
  },
  "author": "Pablo Mayrgundter",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "lint": "eslint src",
    "precommit": "yarn lint && yarn test",
    "serve": "webpack-dev-server --mode=development --port 3000 --hot",
    "test": "jest",
    "prepare-husky": "husky install"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^16.8.0",
    "react-router-dom": "^6.0.2",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.9",
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-import-assertions": "^7.16.7",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.0",
    "babel-jest": "^27.4.4",
    "babel-loader": "^8.2.5",
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "express": "^4.14.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^27.4.3",
    "jsdom": "^20.0.0",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3"
  },
  "jest": {
    "verbose": false,
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [],
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [],
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy",
      "^react($|/.+)": "<rootDir>/node_modules/react$1"
    },
    "setupFilesAfterEnv": []
  }
}
